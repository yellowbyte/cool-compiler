main
var a, b;
{
    let a <- call InputNum();
    let b <- call InputNum();

	if a < b
	then
		let a <- a + 1;
		if a < b
		then
			let b <- a + 1;
		else
			let b <- a + 2;
		fi;
	else
		let a <- a + 2;
		if a < b
		then
			let b <- a + 1;
		else
			let b <- a + 2;
		fi;
	fi;

	call OutputNum(a);
	call OutputNum(b);
}.
